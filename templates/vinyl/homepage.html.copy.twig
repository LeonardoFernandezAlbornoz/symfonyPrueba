{% extends "base.html.twig" %}

{% block title %}
    Create a new record | {{parent()}}
{% endblock %}
{% block body %}
    
<h1>{{title}}</h1>

<div>
    Tracks:
    <ul>
    {% for track in tracks %}
        <li>{{ track.artist }}-{{ track.song|upper }}</li>
    {% endfor %}
    </ul>
    
</div>

{% endblock %}